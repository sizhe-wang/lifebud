{
  "name": "@mastra/client-js",
  "version": "1.0.0-beta.23",
  "description": "The official TypeScript library for the Mastra Client API",
  "author": "",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mastra-ai/mastra.git",
    "directory": "client-sdks/client-js"
  },
  "bugs": {
    "url": "https://github.com/mastra-ai/mastra/issues"
  },
  "homepage": "https://mastra.ai",
  "license": "Apache-2.0",
  "dependencies": {
    "@lukeed/uuid": "^2.0.1",
    "json-schema": "^0.4.0",
    "@mastra/schema-compat": "1.0.0-beta.6",
    "@mastra/core": "1.0.0-beta.23"
  },
  "peerDependencies": {
    "zod": "^3.25.0 || ^4.0.0"
  },
  "devDependencies": {
    "@ai-sdk/ui-utils": "^1.2.11",
    "@types/json-schema": "^7.0.15",
    "@types/node": "22.13.17",
    "@vitest/coverage-v8": "4.0.12",
    "@vitest/ui": "4.0.12",
    "eslint": "^9.37.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "4.0.16",
    "zod": "^3.25.76",
    "@internal/ai-sdk-v4": "0.0.0",
    "@internal/ai-sdk-v5": "0.0.0",
    "@internal/lint": "0.0.53",
    "@internal/types-builder": "0.0.28"
  },
  "engines": {
    "node": ">=22.13.0"
  },
  "scripts": {
    "lint": "eslint .",
    "build:lib": "tsup --config tsup.config.ts",
    "build:docs": "pnpx tsx ../../scripts/generate-package-docs.ts client-sdks/client-js",
    "dev": "pnpm build:lib --watch",
    "test": "vitest",
    "test:unit": "vitest run"
  }
